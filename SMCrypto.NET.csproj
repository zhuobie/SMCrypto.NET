<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Authors>zhuobie</Authors>
    <Company>Inspur</Company>
    <Description>China's standards of encryption algorithms(SM2/SM3/SM4).</Description>
    <Copyright></Copyright>
    <Version>1.1.4</Version>
    <PackageProjectUrl>https://github.com/zhuobie/smcrypto</PackageProjectUrl>
    <RepositoryUrl>https://github.com/zhuobie/smcrypto</RepositoryUrl>
    <PackageTags>Cipher;Crypto;SM2;SM3;SM4</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <None Update="runtimes/**" Pack="true" PackagePath="./">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>
